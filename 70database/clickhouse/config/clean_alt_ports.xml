<?xml version="1.0"?>
<clickhouse>
  <logger>
    <level>information</level>
    <log>./logs/server.log</log>
    <errorlog>./logs/server.err</errorlog>
  </logger>

  <http_port>8231</http_port>
  <tcp_port>9009</tcp_port>

  <path>./data/</path>
  <tmp_path>/tmp/clickhouse/</tmp_path>
  <user_files_path>./user_files/</user_files_path>

  <listen_host>127.0.0.1</listen_host>

  <users>
    <default>
      <password></password>
      <networks>
        <ip>127.0.0.1</ip>
        <ip>::1</ip>
      </networks>
      <profile>default</profile>
      <quota>default</quota>
    </default>
  </users>

  <profiles>
    <default>
      <max_memory_usage>10000000000</max_memory_usage>
    </default>
  </profiles>
</clickhouse>
