{
  "id": null,
  "uid": "alpine-analytics",
  "title": "Alpine Analytics - Production Overview",
  "tags": [
    "alpine",
    "trading",
    "argo"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Argo Win Rate",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "argo_win_rate",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "text": {
          "titleSize": 20,
          "valueSize": 60
        },
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Total Signals",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "argo_total_signals",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "blue"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Sharpe Ratio",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "argo_sharpe_ratio",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 1.5,
                "color": "yellow"
              },
              {
                "value": 2,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Max Drawdown",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "argo_max_drawdown",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": -10,
                "color": "yellow"
              },
              {
                "value": -15,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "CPU Usage",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "legendFormat": "CPU Usage",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percent",
          "max": 100,
          "min": 0
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 6,
      "title": "Memory Usage",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
          "legendFormat": "Memory Usage",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percent",
          "max": 100,
          "min": 0
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 7,
      "title": "Disk Usage",
      "type": "gauge",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"}) * 100)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 70,
                "color": "yellow"
              },
              {
                "value": 85,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Database Connections",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 16
      },
      "targets": [
        {
          "expr": "pg_stat_database_numbackends{datname=\"alpine_prod\"}",
          "legendFormat": "Active Connections",
          "refId": "A"
        }
      ]
    },
    {
      "id": 9,
      "title": "Redis Connected Clients",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 16
      },
      "targets": [
        {
          "expr": "redis_connected_clients",
          "legendFormat": "Connected Clients",
          "refId": "A"
        }
      ]
    }
  ]
}
